<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<assembly targetNamespace="leonarduk">
	<id>kit</id>
	<formats>
		<format>zip</format>
	</formats>
	<fileSets>
		<fileSet>
			<includes>
				<include>conf/**/*</include>
			</includes>
		</fileSet>
		<fileSet>
			<includes>
				<include>logs/.dir</include>
			</includes>
		</fileSet>		
	</fileSets>
	<dependencySets>
		<dependencySet>
			<outputDirectory>lib</outputDirectory>
			<unpack>false</unpack>
			<scope>runtime</scope>
		</dependencySet>
	</dependencySets>
</assembly>